 <asx:abap xmlns:asx="http://www.sap.com/abapxml" version="1.0">
  <asx:values>
   <VSEOCLASS>
    <CLSNAME>ZCL_EA_ALV_TABLE_COLUMNS</CLSNAME>
    <LANGU>E</LANGU>
    <DESCRIPT>Class for building field catalogue table for &lt;em&gt;cl_gui_alv_</DESCRIPT>
    <STATE>1</STATE>
    <CLSCCINCL>X</CLSCCINCL>
    <FIXPT>X</FIXPT>
    <UNICODE>X</UNICODE>
   </VSEOCLASS>
   <DESCRIPTIONS>
    <SEOCOMPOTX>
     <CMPNAME>BUILD_FC_FROM_TABLE</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Uses &lt;em&gt;cl_salv_data_descr=&gt;read_structdescr&lt;/em&gt;</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>MOVE_COLUMN</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Rearranges columns and recalculates col_pos</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>SET_AS_CELL_STYLE</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Get styles from &lt;em&gt;cl_gui_alv_grid&lt;/em&gt;, e.g. &lt;em&gt;cl_gui_al</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>SET_AS_EXCEPTION</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Check documentation for &lt;em&gt;cl_salv_columns_list=&gt;set_except</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>SET_DDIC_FIELD</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Mark column as field from table that has foreign key, in ord</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>SET_OPTIMIZE</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Warning! Can slow down display if there is too many rows/col</DESCRIPT>
    </SEOCOMPOTX>
   </DESCRIPTIONS>
  </asx:values>
 </asx:abap>
